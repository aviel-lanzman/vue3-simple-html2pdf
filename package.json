{
  "name": "vue3-to-pdf",
  "description": "Export to PDF using vue3 and html2pdf.js",
  "version": "1.0.7",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-to-pdf.umd.cjs",
  "module": "./dist/vue3-to-pdf.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-to-pdf.js",
      "require": "./dist/vue3-to-pdf.umd.cjs"
    }
  },
  "author": "Aviel Lanzman <avielnzmn@gmail.com>",
  "contributors": [
    "Aviel Lanzman <avielnzmn@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "vue3 pdf",
    "vue3 to pdf",
    "pdf vue3",
    "vue html2pdf",
    "vue3-to-pdf",
    "export pdf"
  ],
  "scripts": {
    "build": "vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviel-lanzman/vue3-simple-html2pdf.git"
  },
  "dependencies": {
    "html2pdf.js": "^0.9.2",
    "jspdf": "^1.4.1",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.1",
    "vite": "^4.4.9",
    "vue": "^3.3.4"
  },
  "bugs": {
    "url": "https://github.com/aviel-lanzman/vue3-simple-html2pdf"
  },
  "bin": {
    "install": "src/index.js"
  },
  "homepage": "https://github.com/aviel-lanzman/vue3-simple-html2pdf"
}
